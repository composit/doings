<%= content_for( :header ) do %>
  <h1>Projects</h1>
<% end %>

<% for project in @projects %>
  <div class="project" id="project-<%= project.id %>">
    <%= project.name %>
    <div class="details">
      <%= link_to "tickets", "#", :class => "project-details", "data-project-id" => project.id %>
    </div>
  </div>
<% end %>
